function output = KNN ( Coordinates, K, data, class_labels)
    minval = ones(1,K);
    minidxs = [];
    dist = zeros(1,length(data));
    for idx = 1:length(data)
        dist(idx) = norm(data(idx,:) - Coordinates);
        %tempmin = norm(data(idx,:) - Coordinates);
        %if tempmin < max(minval)
        %    minval(K) = tempmin;
        %    minval = sort(minval);
        %    minidxs = [minidxs idx]; 
        %end 
    end 
    
    [B, I] = sort(dist,'ascend');

    I(1:K)

    total = class_labels(minidxs(end-(K-1):end));

    output = mode(total);
end
